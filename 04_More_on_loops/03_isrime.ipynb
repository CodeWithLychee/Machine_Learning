   "source": [
    "n = int(input(\"Enter the value of n : \"))\n",
    "isprime=True\n",
    "for i in range(2,n):\n",
    "    if n%i==0:\n",
    "        isprime=False\n",
    "        break\n",
    "\n",
    "if isprime:\n",
    "    print(\"Number is prime\")\n",
    "else:\n",
    "    print(\"Not prime\")"
